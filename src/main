require_relative 'coordinates'
require_relative 'snake'
require_relative 'player'

start = []
for i in 1..4
    start.push(Coordinates.new(i, 1))
end
s = Snake.new(start)
s.position.fifo_print
s.move(s.next_coordinate('LEFT'))
s.position.fifo_print
s.move(s.next_coordinate('RIGHT'))
s.position.fifo_print
