require_relative 'coordinates'
require_relative 'snake'
require_relative 'player'

p = Player.new('Dave', 0)
start = []
for i in 1..4
    start.push(Coordinates.new(i, i+1))
end
s = Snake.new(p, start)
print s.name
s.position.fifo_print

s.move(4,true)
s.move(Coordinates.new(6,7))
s.move(Coordinates.new(7,8), true)
s.move(Coordinates.new(9,3), true, true)
s.move(Coordinates.new(11,11))

s.position.fifo_print
